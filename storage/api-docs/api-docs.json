    "components": {
        "schemas": {
            "StoreDocumentRequest": {
                "title": "Store Document Request",
                "description": "Yêu cầu lưu tài liệu mới",
                "required": [
                    "document"
                ],
                "properties": {
                    "document": {
                        "description": "Tệp tài liệu cần tải lên (PDF, DOC, DOCX, XLSX, XLS, PNG, JPG)",
                        "type": "string",
                        "format": "binary",
                        "example": "file.pdf"
                    }
                },
                "type": "object"
            },
            "StoreRoleRequest": {
                "title": "Store Role Request",
                "description": "Yêu cầu tạo mới vai trò",
                "required": [
                    "roleName",
                    "roleCode",
                    "permissions"
                ],
                "properties": {
                    "roleName": {
                        "description": "Tên vai trò.",
                        "type": "string",
                        "example": "Quản trị viên"
                    },
                    "roleCode": {
                        "description": "Mã vai trò duy nhất.",
                        "type": "string",
                        "example": "ADMIN001"
                    },
                    "permissions": {
                        "description": "Danh sách quyền được cấp cho vai trò.",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "example": "1.1"
                        }
                    }
                },
                "type": "object"
            },
            "StoreUserRequest": {
                "title": "Store User Request",
                "description": "Yêu cầu tạo mới người dùng",
                "required": [
                    "name",
                    "email",
                    "password",
                    "passwordConfirmation",
                    "phoneNumber",
                    "roleId",
                    "status"
                ],
                "properties": {
                    "name": {
                        "description": "Tên đầy đủ của người dùng.",
                        "type": "string",
                        "example": "Thành Đạt"
                    },
                    "email": {
                        "description": "Địa chỉ email của người dùng.",
                        "type": "string",
                        "format": "email",
                        "example": "thanhdat@example.com"
                    },
                    "phoneNumber": {
                        "description": "Số điện thoại của người dùng.",
                        "type": "string",
                        "example": "0399839848"
                    },
                    "avatar": {
                        "description": "Ảnh đại diện của người dùng (file ảnh).",
                        "type": "file"
                    },
                    "roleId": {
                        "description": "ID vai trò của người dùng.",
                        "type": "integer",
                        "example": 1
                    },
                    "status": {
                        "description": "Trạng thái tài khoản (true: hoạt động, false: bị khóa).",
                        "type": "boolean",
                        "example": true
                    },
                    "password": {
                        "description": "Mật khẩu của người dùng, tối thiểu 8 ký tự.",
                        "type": "string",
                        "format": "password",
                        "example": "password123"
                    },
                    "passwordConfirmation": {
                        "description": "Xác nhận mật khẩu của người dùng.",
                        "type": "string",
                        "format": "password",
                        "example": "password123"
                    }
                },
                "type": "object"
            },
            "UpdateRoleRequest": {
                "title": "Update Role Request",
                "description": "Yêu cầu cập nhật vai trò",
                "properties": {
                    "roleName": {
                        "description": "Tên vai trò.",
                        "type": "string",
                        "example": "Quản trị viên"
                    },
                    "permissions": {
                        "description": "Danh sách quyền được cấp cho vai trò.",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "example": "1.1"
                        }
                    }
                },
                "type": "object"
            },
            "UpdateUserRequest": {
                "title": "Update User Request",
                "description": "Yêu cầu cập nhật thông tin người dùng",
                "required": [
                    "_method",
                    "password",
                    "passwordConfirmation",
                    "phoneNumber",
                    "roleId",
                    "status"
                ],
                "properties": {
                    "_method": {
                        "description": "Giả lập phương thức PUT để cập nhật tài nguyên.",
                        "type": "string",
                        "example": "PUT"
                    },
                    "phoneNumber": {
                        "description": "Số điện thoại của người dùng.",
                        "type": "string",
                        "example": "0123456789"
                    },
                    "avatar": {
                        "description": "Ảnh đại diện của người dùng (file ảnh).",
                        "type": "string",
                        "format": "binary"
                    },
                    "roleId": {
                        "description": "ID của vai trò mới của người dùng.",
                        "type": "integer",
                        "example": 2
                    },
                    "status": {
                        "description": "Trạng thái tài khoản (true: hoạt động, false: bị khóa).",
                        "type": "boolean",
                        "example": true
                    },
                    "password": {
                        "description": "Mật khẩu mới, tối thiểu 8 ký tự, cần nhập lại để xác nhận.",
                        "type": "string",
                        "format": "password",
                        "example": "newpassword123"
                    },
                    "passwordConfirmation": {
                        "description": "Xác nhận mật khẩu mới của người dùng.",
                        "type": "string",
                        "format": "password",
                        "example": "newpassword123"
                    }
                },
                "type": "object"
            },
            "Document": {
                "title": "Document",
                "description": "Chi tiết tài liệu",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "fileName": {
                        "type": "string",
                        "example": "document.pdf"
                    },
                    "fileExtension": {
                        "type": "string",
                        "example": "pdf"
                    },
                    "filePath": {
                        "type": "string",
                        "example": "documents/document.pdf"
                    }
                },
                "type": "object"
            },
            "Permission": {
                "title": "Permission",
                "description": "Thông tin quyền",
                "properties": {
                    "id": {
                        "description": "ID của quyền.",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Tên của quyền.",
                        "type": "string",
                        "example": "Xem người dùng"
                    }
                },
                "type": "object"
            },
            "Role": {
                "title": "Role",
                "description": "Thông tin vai trò",
                "properties": {
                    "id": {
                        "description": "ID của vai trò.",
                        "type": "integer",
                        "example": 1
                    },
                    "roleCode": {
                        "description": "Mã của vai trò.",
                        "type": "string",
                        "example": "ADMIN001"
                    },
                    "roleName": {
                        "description": "Tên của vai trò.",
                        "type": "string",
                        "example": "Quản trị viên"
                    },
                    "permissions": {
                        "description": "Danh sách quyền của vai trò",
                        "type": "array",
                        "items": {
                            "properties": {
                                "groupName": {
                                    "description": "Tên nhóm quyền.",
                                    "type": "string",
                                    "example": "Quản lý người dùng"
                                },
                                "permissions": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Permission"
                                    }
                                }
                            },
                            "type": "object"
                        }
                    }
                },
                "type": "object"
            },
            "User": {
                "title": "User",
                "description": "Thông tin người dùng",
                "properties": {
                    "id": {
                        "description": "ID của người dùng.",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Tên đầy đủ của người dùng.",
                        "type": "string",
                        "example": "Nguyễn Văn A"
                    },
                    "email": {
                        "description": "Địa chỉ email của người dùng.",
                        "type": "string",
                        "format": "email",
                        "example": "user@example.com"
                    },
                    "phoneNumber": {
                        "description": "Số điện thoại của người dùng.",
                        "type": "string",
                        "example": "0123456789"
                    },
                    "role": {
                        "description": "Thông tin vai trò của người dùng",
                        "properties": {
                            "id": {
                                "description": "ID vai trò của người dùng.",
                                "type": "integer",
                                "example": 2
                            },
                            "roleCode": {
                                "description": "Mã vai trò của người dùng.",
                                "type": "string",
                                "example": "USER001"
                            },
                            "roleName": {
                                "description": "Tên vai trò của người dùng.",
                                "type": "string",
                                "example": "User"
                            }
                        },
                        "type": "object"
                    },
                    "status": {
                        "description": "Trạng thái tài khoản của người dùng (true: hoạt động, false: bị khóa).",
                        "type": "boolean",
                        "example": true
                    },
                    "avatar": {
                        "description": "Đường dẫn ảnh đại diện của người dùng.",
                        "type": "string",
                        "example": "https://example.com/avatar.jpg",
                        "nullable": true
                    },
                    "lastLogin": {
                        "description": "Thời gian đăng nhập lần cuối của người dùng.",
                        "type": "string",
